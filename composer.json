{
    "name": "kunstmaan/cms-skeleton",
    "type": "project",
    "description": "A skeleton to start your new Kunstmaan CMS website",
    "keywords": ["cms"],
    "homepage": "https://kunstmaancms.be",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan CMS Team",
            "email": "cms@kunstmaan.be"
        },
        {
            "name": "The Community",
            "homepage": "https://github.com/Kunstmaan/KunstmaanBundlesCMS/graphs/contributors"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^2.2.2"
    },
    "flex-require": {
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/doctrine-bundle": "^2.4",
        "egulias/email-validator": "^1.2.8|^2.0",
        "friendsofsymfony/http-cache-bundle": "^2.6",
        "kunstmaan/admin-bundle": "7.0-dev",
        "kunstmaan/adminlist-bundle": "7.0-dev",
        "kunstmaan/article-bundle": "7.0-dev",
        "kunstmaan/config-bundle": "7.0-dev",
        "kunstmaan/dashboard-bundle": "7.0-dev",
        "kunstmaan/form-bundle": "7.0-dev",
        "kunstmaan/media-bundle": "7.0-dev",
        "kunstmaan/media-pagepart-bundle": "7.0-dev",
        "kunstmaan/menu-bundle": "7.0-dev",
        "kunstmaan/node-bundle": "7.0-dev",
        "kunstmaan/node-search-bundle": "7.0-dev",
        "kunstmaan/pagepart-bundle": "7.0-dev",
        "kunstmaan/redirect-bundle": "7.0-dev",
        "kunstmaan/search-bundle": "7.0-dev",
        "kunstmaan/seo-bundle": "7.0-dev",
        "kunstmaan/sitemap-bundle": "7.0-dev",
        "kunstmaan/translator-bundle": "7.0-dev",
        "kunstmaan/user-management-bundle": "7.0-dev",
        "kunstmaan/utilities-bundle": "7.0-dev",
        "nyholm/psr7": "^1.1",
        "php-http/guzzle7-adapter": "^1.0",
        "sensio/framework-extra-bundle": "^6.2",
        "sentry/sentry-symfony": "^4.2",
        "symfony/apache-pack": "*",
        "symfony/asset": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/expression-language": "^5.4",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^5.4",
        "symfony/runtime": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/web-link": "^5.4",
        "symfony/webpack-encore-bundle": "^1.15",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
    },
    "flex-require-dev": {
        "kunstmaan/generator-bundle": "7.0-dev",
        "kunstmaan/fixtures-bundle": "7.0-dev",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "suggest": {
        "phpro/grumphp": "A PHP code-quality tool",
        "friendsofphp/php-cs-fixer": "",
        "wearejust/grumphp-extra-tasks": "Automate your quality with PHP CS auto fixer"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": [],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "kunstmaan/bundles-cms": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
